{
  "name": "photo-open-call-analyzer",
  "version": "1.0.0",
  "description": "AI-powered tool to analyze photos for photography open calls and generate rankings",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "analyze": "node src/cli/analyze.js",
    "report": "node src/cli/report.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:jest-compat": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "photography",
    "open-call",
    "ai",
    "ollama",
    "llava",
    "image-analysis",
    "ranking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "ollama": "^0.5.11",
    "ora": "^8.1.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jest": "^29.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
