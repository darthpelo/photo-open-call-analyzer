{
  "name": "photo-open-call-analyzer",
  "version": "1.0.0",
  "description": "AI-powered tool to analyze photos for photography open calls and generate rankings",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "analyze": "node src/cli/analyze.js",
    "report": "node src/cli/report.js",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "photography",
    "open-call",
    "ai",
    "claude",
    "image-analysis",
    "ranking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "sharp": "^0.33.5",
    "exif-reader": "^2.0.1",
    "commander": "^12.1.0",
    "ora": "^8.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^9.15.0",
    "prettier": "^3.4.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
